{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
    "rewrites": [{ "source": "/(.*)", "destination": "server.js" }],
    "crons": [{
      "path": "./server.js",
      "schedule": "0 0 0 5 * *"
    }]
}